<%= form_for @user_event do |f| %>

  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :location %>
  <%= f.text_field :location %>

  <%= f.collection_select(:user_id, @users, :id, :user_name) %>

  <%= f.submit %>
<% end %>
